{
  "name": "portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "start:host": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "build:stag": "ng build --configuration stag",
    "build:dev": "ng build --configuration dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "icons": "svg-sprite --config sprite-config.json src/assets/icons/svg/normal/*.svg && svg-sprite --config colors-sprite-config.json src/assets/icons/svg/color/*.svg",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.html\" \"src/**/*.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@ckeditor/ckeditor5-angular": "^7.0.1",
    "@ckeditor/ckeditor5-build-classic": "^40.2.0",
    "@ckeditor/ckeditor5-build-decoupled-document": "^40.2.0",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/entity": "^17.0.1",
    "@ngrx/store": "^17.0.1",
    "@ngx-dropzone/cdk": "^17.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "dayjs": "^1.11.10",
    "ng-zorro-antd": "17.1.0",
    "ngx-cookie-service": "^17.0.1",
    "ngx-countdown": "^17.1.1",
    "ngx-drag-drop": "^17.0.0",
    "ngx-dropzone": "^3.1.0",
    "ngx-image-compress": "^15.1.6",
    "ngx-image-cropper": "^8.0.0",
    "ngx-scrollbar": "^13.0.3",
    "ngx-webstorage": "^13.0.1",
    "rxjs": "~7.8.0",
    "svg-sprite": "^2.0.2",
    "swiper": "^11.0.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.2.2"
  }
