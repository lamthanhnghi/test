{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@core/*": [
        "./src/app/core/*",
        "./src/app/core/configs/*",
        "./src/app/core/interceptors/*",
        "./src/app/core/guards/*"
      ],
      "@shared/*": [
        "./src/app/shared/*",
        "./src/app/shared/components/*",
        "./src/app/shared/directives/*",
        "./src/app/shared/pipes/*",
        "./src/app/shared/services/*",
        "./src/app/shared/models/*",
        "./src/app/shared/utils/*",
        "./src/app/shared/stores/*"
      ],
      "@modules/*": [
        "./src/app/modules/*"
      ],
      "@modules/home/*": [
        "./src/app/modules/home/*"
      ],
      "@modules/account/*": [
        "./src/app/modules/account/*"
      ],
      "@modules/profile-management/*": [
        "./src/app/modules/profile-management/*"
      ],
      "@modules/products/*": [
        "./src/app/modules/products/*"
      ],
      "@modules/general-setting/*": [
        "./src/app/modules/general-setting/*"
      ],
      "@modules/agreements/*": [
        "./src/app/modules/agreements/*"
      ],
      "@modules/categories/*": [
        "./src/app/modules/categories/*"
      ],
      "@modules/product-groups/*": [
        "./src/app/modules/product-groups/*"
      ],
      "@modules/delivery-fee-policies/*": [
        "./src/app/modules/delivery-fee-policies/*"
      ],
      "@modules/dept-policies/*": [
        "./src/app/modules/dept-policies/*"
      ],
      "@modules/notification-management/*": [
        "./src/app/modules/notification-management/*"
      ],
      "@modules/page-not-found/*": [
        "./src/app/modules/page-not-found/*"
      ],
      "@env/*": [
        "./src/environments/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "extendedDiagnostics": {
      "checks": {
        "optionalChainNotNullable": "suppress"
      }
    }
  }
}
